<%- include ("../eShopPartials/header", {title:title});  %>  <!-- kommer synas på skärmen som html... -->

<h1> All Orders / Beställningar</h1>

<table>
    <th>order_id</th>
    <th>customer_id</th>
    <th>customer_Name</th>

    <th>date_of_order</th>
    <th>ordered_quantity</th>
    <th>status</th>
    <th></th>
    <% orders.forEach(function(order) {%>

        <tr>
            <td><%= order.order_id %></td>
            <td><%= order.customer_id %></td>
            <td><%= order.name %></td>
            <td><%= order.date_of_order %></td>
            <td><%= order.ordered_quantity %></td>
            <td><%= order.status %></td>
            <td><button><a href="pickList/<%= order.order_id %>">access picklist</a></button></td>
        </tr>
    <%    });    %>
</table>


<%- include("../eShopPartials/footer"); %>

